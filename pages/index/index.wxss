}
.likes_wrap{
  flex: 1;
  /* height: max-content; */
  box-sizing: border-box;
  padding: 2rpx 30rpx;
  display: flex;
  /* flex-direction: row; */
  align-items: center;
  /* border-bottom: 1rpx solid white; */
  /* flex-wrap: wrap; */
