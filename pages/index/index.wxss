.homeHeader .swiper {
  width: 100%;
  height: 100%;
}

.homeHeader .wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: 85rpx;
}

.homeHeader .wx-swiper-dot {
  width: 40rpx;
  display: inline-flex;
  height: 10rpx;
  /* margin-left: 20rpx; */
  justify-content: space-between;
}

.homeHeader .wx-swiper-dot::before {
  content: '';
  flex-grow: 1;
  background: white;
  border-radius: 8rpx;
}

.homeHeader .wx-swiper-dot-active::before {
  background: #f66e26;
}

.filterContent {
