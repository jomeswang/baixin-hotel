.roomCard {
  width: 100%;
  height: 272rpx;
  display: flex;
}
.roomCard .roomImg_wrap {
  flex: 1;
  background-color: red;
}
.roomCard .roomImg_wrap .roomImg {
  width: 228rpx;
  height: 208rpx;
}
.roomCard .rightContent {
  flex: 2.8;
  background-color: green;
  flex-direction: row;
}
.roomCard .rightContent .roomName {
  background: #FFFFFF;
  box-shadow: 0px 0px 6px rgba(0, 0, 0, 0.16);
  font-size: 32rpx;
  font-weight: 400;
}


.roomCard .rightContent .roomThird {
  flex-direction: row;
}
